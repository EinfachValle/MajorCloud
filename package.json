{
  "name": "neat-beat",
  "version": "1.0.0",
  "private": true,
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "author": "Valentin RÃ¶hle",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "lint": "vue-cli-service lint",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit:dev": "cypress open --component"
  },
  "main": "babel.config.js",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@vue/babel-preset-app": "^5.0.8",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "bootstrap-vue": "^2.23.1",
    "browserslist": "^4.21.5",
    "core-js": "^3.29.1",
    "crypto-browserify": "^3.12.0",
    "dot-object": "^2.1.4",
    "file-uri-to-path": "^2.0.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.1",
    "js-logger": "^1.6.1",
    "lint-staged": "^13.2.0",
    "mdb-ui-kit": "^6.2.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.42",
    "pinia": "^2.0.32",
    "swiper": "^9.1.1",
    "validator": "^13.9.0",
    "vue": "^3.2.47",
    "vue-fuse": "^4.1.1",
    "vue-i18n": "^9.2.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-i18n-routing": "^0.12.2",
    "vue-loading-overlay": "^6.0.3",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "css-loader": "^6.7.3",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "prettier": "^2.8.4",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.2",
    "vue-cli-plugin-i18n": "^2.3.2",
    "vue-cli-plugin-vue-i18n": "~1.0.1",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.2",
    "webpack-hot-middleware": "^2.25.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "actions/**/*.js",
      "initializers/**/*.js",
      "servers/**/*.js",
      "tasks/**/*.js",
      "src/**/*.{vue}",
      "!**/node_modules/**"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/actions/",
      "/initializers/",
      "/servers/",
      "/tasks/"
    ],
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "bugs": {
    "url": "https://github.com/EinfachValle/NeatBeat/issues"
  },
  "homepage": "https://github.com/EinfachValle/NeatBeat#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "Music",
    "coding",
    "message"
  ],
  "license": "ISC",
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet --fix --ext .js,.vue src/",
      "git add"
    ],
    "**/*.{vue,html,js,json,scss,less,css,md}": [
      "prettier --config ./.prettierrc --ignore-path ./.eslintignore --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EinfachValle/NeatBeat.git"
  }
}
